    public class second : Program 
    {
        public static void Rend()
        {
            for (int l = tm - 1; l > 0; l--)
            {
                for (int p = 0; p < l; p++)
                {
                    if (tomb[p] > tomb[p + 1])
                    {
                        int x = tomb[p + 1];
                        tomb[p + 1] = tomb[p];
                        tomb[p] = x;
                    }
                }
            }
        }
    }
